{
  "name": "@flowi/core",
  "version": "0.0.0",
  "description": "Flowi Core",
  "main": "./dist/index.js",
  "repository": "git@github.com:AlboeDev/flowi-ecs.git",
  "author": "Alboe Development",
  "license": "MIT",
  "private": false,
  "scripts": {
    "docs:build": "api-documenter markdown -i ./docs/data -o ./docs",
    "docs:clean": "rimraf ./docs",
    "docs:extract": "api-extractor run -c ./config/api-extractor.config.json",
    "prepare": "husky install config/husky",
    "dist:build": "tsc",
    "dist:clean": "rimraf ./dist"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@microsoft/api-documenter": "^7.13.77",
    "@microsoft/api-extractor": "^7.19.2",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
}
